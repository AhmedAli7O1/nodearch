{
  "name": "@nodearch/cli",
  "version": "1.2.4",
  "description": "nodearch cli",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "nodearch": "./dist/start.js"
  },
  "scripts": {
    "build": "rimraf ./dist/ && tsc",
    "build:watch": "tsc",
    "release": "npm run build && npm publish --tag beta --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluemax-io/nodearch.git"
  },
  "keywords": [
    "nodearch",
    "nodearch-cli"
  ],
  "author": "https://github.com/AhmedAli7O1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bluemax-io/nodearch/issues",
    "email": "bluemax7o1@gmail.com"
  },
  "dependencies": {
    "@nodearch/core": "workspace:^1.4.2",
    "axios": "^0.21.0",
    "decompress": "^4.2.1",
    "node-notifier": "^8.0.0",
    "open": "^8.4.0",
    "ora": "^5.4.1",
    "cli-spinners": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "yargs": "^16.1.1",
    "json5": "^2.2.1",
    "@types/mocha": "^10.0.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@types/node-notifier": "^8.0.0",
    "@types/decompress": "^4.2.3",
    "@types/node": "^16.18.0",
    "@types/nodemon": "^1.19.2",
    "@types/yargs": "^15.0.9",
    "nodemon": "^2.0.20",
    "rimraf": "~3.0.2"
  },
  "nodearch": {
    "paths": {
      "root": "./",
      "app": "./src/main.ts"
    }
  }
}
