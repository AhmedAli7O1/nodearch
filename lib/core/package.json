  "bugs": {
    "url": "https://github.com/nodearch/nodearch/issues",
    "email": "bluemax7o1@gmail.com"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "reflect-metadata": "^0.1.13",
    "typescript": "^5.2.2",
    "rimraf": "~3.0.2",
    "mocha": "^10.2.0",
    "@types/mocha": "^10.0.1"
  },
  "dependencies": {
    "inversify": "6.0.1"
  },
  "peerDependencies": {
    "reflect-metadata": "0.x"
  }
