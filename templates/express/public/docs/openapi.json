{
  "openapi": "3.0.0",
  "info": {
    "title": "NodeArch Express Template",
    "version": "0.1.0"
  },
  "paths": {
    "/product": {
      "get": {
        "summary": "getProducts",
        "description": "Get Products",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/product/{id}": {
      "get": {
        "summary": "getProductById",
        "description": "Get Product By Id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/test": {
      "get": {
        "summary": "test",
        "description": "Test",
        "parameters": [
          {
            "name": "username",
            "in": "query",
            "schema": {
              "type": "string",
              "minLength": 10
            }
          },
          {
            "name": "role",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": [
                "admin",
                "user"
              ]
            }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "tags": [
          "User Management",
          "Special Tag"
        ]
      }
    },
    "/": {
      "get": {
        "summary": "getUsers",
        "description": "Get Users",
        "parameters": [],
        "responses": {
          "200": {
            "description": "User response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "number"
                    },
                    "name": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "tags": [
          "User Management",
          "Special Tag"
        ]
      },
      "post": {
        "summary": "addUser",
        "description": "Add User",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "User name",
                    "example": "John Doe"
                  },
                  "age": {
                    "type": "number",
                    "format": "float"
                  }
                },
                "required": [
                  "name",
                  "age"
                ],
                "additionalProperties": false
              }
            }
          },
          "description": "User request body"
        },
        "tags": [
          "User Management",
          "Special Tag"
        ]
      }
    },
    "/{id}": {
      "get": {
        "summary": "getUserById",
        "description": "Get User By Id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "schema": {
              "type": "string",
              "minLength": 5,
              "maxLength": 10
            }
          },
          {
            "name": "x-api-key",
            "in": "cookie",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "something": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "tags": [
          "User Management",
          "Special Tag"
        ],
        "requestBody": {
          "description": "User request body",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "User name",
                    "example": "John Doe"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "http://localhost:6000",
      "description": "Local server O"
    },
    {
      "url": "http://localhost:3000",
      "description": "Express Server URL"
    },
    {
      "url": "http://localhost:3000",
      "description": "Local server"
    },
    {
