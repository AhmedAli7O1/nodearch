    "email": "bluemax7o1@gmail.com"
  },
  "dependencies": {
    "@nodearch/command": "workspace:^1.1.0"
  },
  "devDependencies": {
    "@nodearch/core": "workspace:^2.2.0",
    "typescript": "^5.7.3",
    "@types/node": "^22.10.10",
    "rimraf": "^6.0.1",
    "@types/mocha": "^10.0.10",
    "mocha": "^11.1.0"
  },
  "peerDependencies": {
    "@nodearch/core": "2.2.0",
    "mocha": "10.x",
