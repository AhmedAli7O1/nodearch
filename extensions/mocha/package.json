  },
  "dependencies": {
    "@nodearch/core": "workspace:^1.4.2",
    "@nodearch/command": "workspace:^0.1.0",
    "typescript": "^5.2.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "rimraf": "~3.0.2",
    "@types/mocha": "^10.0.1"
